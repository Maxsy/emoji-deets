{
  "name": "emoji-deets",
  "version": "0.0.0",
  "description": "Get details about emojis",
  "main": "./lib/index.mjs",
  "type": "module",
  "types": "./index.d.ts",
  "module": "./lib/index.mjs",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./index.d.ts"
    },
    "./package.json": "./package.json",
    "./locales/*": "./lib/locales/*"
  },
  "files": [
    "/lib",
    "/index.d.ts"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf lib/*",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "release": "npm run build && changeset publish",
    "build:esm": "rollup --config rollup.config.js",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "rollup": "rollup --config rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Maxsy/emoji-deets.git"
  },
  "keywords": [
    "Emoji",
    "Details",
    "emoji"
  ],
  "author": "maxii.zex@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Maxsy/emoji-deets/issues"
  },
  "homepage": "https://github.com/Maxsy/emoji-deets#readme",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-typescript": "^10.0.1",
    "rollup": "^3.7.5",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
